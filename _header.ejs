  <script>
    $(document).ready(function () {
      $(window).scroll(function () {
        if ($(this).scrollTop() > 100) {
          $(".scrollup").fadeIn();
        } else {
          $(".scrollup").fadeOut();
        }
      });

      $(".scrollup").click(function () {
        $("html, body").animate(
          {
            scrollTop: 0,
          },
          600
        );

        return false;
      });
    });
  </script>
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

  <script>
    function checkCookie() {
      // Belirli bir Ã§erez var mÄ± kontrol et
      if (document.cookie.indexOf("cookieConsent=true") == -1) {
        // Ã‡erez bildirimini gÃ¶ster
        Swal.fire({
          position: "bottom-start",
          title: "Ã‡erez KullanÄ±mÄ±",
          text: "Bu web sitesi Ã§erezleri kullanÄ±r. Web sitemizi kullanarak Ã§erez kullanÄ±mÄ±nÄ± kabul edersiniz.",
          showCancelButton: true,
          confirmButtonText: "Kabul Et",
          cancelButtonText: "Reddet",
          width: "450",
        }).then((result) => {
          if (result.isConfirmed) {
            // KullanÄ±cÄ± Ã§erez kullanÄ±mÄ±nÄ± kabul etti
            document.cookie = "cookieConsent=true; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/";
          } else {
            // KullanÄ±cÄ± Ã§erez kullanÄ±mÄ±nÄ± kabul etmedi
            document.cookie = "cookieConsent=false; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/";
          }
        });
      }
    }
  
    window.onload = checkCookie;
  </script>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    const scrollUpButton = document.querySelector('.scrollup');

    // Butonu baÅŸlangÄ±Ã§ta gizli yap
    window.addEventListener('scroll', function () {
      if (window.scrollY > 200) {
        scrollUpButton.classList.add('show');
      } else {
        scrollUpButton.classList.remove('show');
      }
    });

    // Butona tÄ±klandÄ±ÄŸÄ±nda sayfanÄ±n en Ã¼stÃ¼ne animasyonlu kaydÄ±r
    scrollUpButton.addEventListener('click', function () {
      window.scrollTo({
        top: 0,
        behavior: 'smooth'
      });
    });
  });
</script>


